.key {
  padding: 0.3rem 0.7rem;
  background: #e6e6e6;
}

.no-data {
  text-align: center;
}

.key-title {
  text-align: center;
  margin-bottom: 1rem;
}

.key-list {
  border-bottom: 1px solid lightgray;
  margin-bottom: 0.5rem;
  padding: 0.5rem;
  background-color: white;
  border-radius: 4px;
}

.key-info>text {
  margin-right: 1rem;
  margin-bottom: 0.8rem;
  font-size: 32rpx;
  display: block;
}

.key-group {
  margin-top: 0.5rem;
}

.key-group>text {
  font-size: 14px;
  color: #db524b !important;
  display: block;
}

.key-group>view {
  font-size: 15px;
  color: #9b9b9b;
  height: 32px;
}

.key-group>view>text {
  width: 80px;
  display: inline-block;
  text-align: left;
}

.key-group>view:last-child {
  text-align: right;
}

.key-group input {
  display: inline-block;
  position: relative;
  top: 8px;
  left: 0.5rem;
  border: 1px solid #9b9b9b;
  border-radius: 4px;
  padding-left: 4px;
}

.active {
  border: none!important;
}
